{
    "manifest_version": 2,
    "name": "Drug Dictionary",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["Content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "Popup.html",
        "default_title": "Drug Dictionary"    
    }
}